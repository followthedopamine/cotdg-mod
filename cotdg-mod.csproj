<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>cotdg_mod</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Memory.dll" Version="1.2.21" />
    <PackageReference Include="System.Security.Principal.Windows" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Fasm.NET">
      <HintPath>Fasm.NET.dll</HintPath>
    </Reference>
    <Reference Include="memory">
      <HintPath>..\MemoryPatternScanner\memory\bin\Release\memory.dll</HintPath>
    </Reference>
    <Reference Include="MemorySharp">
      <HintPath>MemorySharp.dll</HintPath>
    </Reference>
  </ItemGroup>


</Project>
